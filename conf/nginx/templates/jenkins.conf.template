server {
    server_name jenkins.athe.nova;
    listen 80;

    location / {
        proxy_pass http://jenkins:8080;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $host;
	proxy_read_timeout      300;
	proxy_connect_timeout   300;
        proxy_redirect off;
    }
}
